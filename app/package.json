{
  "name": "nextjs-start",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scrapts": {
    "next": "next",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=dev nodemon -e js,graphql,json --exec babel-node src/index.js",
    "start": "node .compiled/src/index.js",
    "start2": "cross-env NODE_ENV=production PORT=80 babel-node server.js",
    "compile": "rm -rf .compiled && babel src --out-dir .compiled/src",
    "buildCSS": "stylus -u autoprefixer-stylus ./styles/main.styl -o ./static/main.css",
    "devCSS": "stylus -u autoprefixer-stylus -w ./styles/main.styl -o ./static/main.css",
    "generateLocales": "cross-env NODE_ENV=dev babel-node generateLocales.js",
    "cleanCaches": "rm -rf .next && npm cache clean --force && rm -rf node_modules/.cache/* && npm i",
    "cleanCache": "rm -rf node_modules/.cache",
    "deploy": "npm run buildCSS; cd .deploy; mupx deploy; cd .."
  },
  "scripts": {
    "build": "gulp build",
    "dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=dev babel-node server.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "next:build": "rm -rf .next && next build",
    "sass:compile": "sass static/sass/main.scss static/css/main.css",
    "pm2:up": "pm2 start npm --name pixel-sushi-app -- run prod",
    "pm2:down": "pm2 stop pixel-sushi-app"
  },
  "quokka": {
    "babel": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-boost": "^0.1.16",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.0.0-rc.7",
    "apollo-link-http": "^1.5.4",
    "bootstrap": "^4.1.3",
    "cookies-js": "^1.2.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "isomorphic-fetch": "^2.2.1",
    "next": "^5.1.0",
    "node-fetch": "^2.2.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.0",
    "react-bootstrap": "^1.0.0-beta.1",
    "react-dom": "^16.4.1",
    "request": "^2.87.0",
    "scrollmagic": "^2.0.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.9.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-run": "^1.7.1",
    "gulp-sftp": "^0.1.5",
    "nodemon": "^1.18.3",
    "run-sequence": "^2.2.1"
  }
}
